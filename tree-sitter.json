{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kcl",
      "camelcase": "KCL",
      "title": "KCL",
      "scope": "source.kcl",
      "file-types": [
        "k"
      ],
      "injection-regex": "^kcl$",
      "class-name": "TreeSitterKcl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "KCL Configration and policy language",
    "authors": [
      {
        "name": "Peefy",
        "email": "xpf6677@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/kcl-lang/tree-sitter-kcl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}